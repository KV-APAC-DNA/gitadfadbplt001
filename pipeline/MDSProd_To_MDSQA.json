{
	"name": "MDSProd_To_MDSQA",
	"properties": {
		"activities": [
			{
				"name": "Copy MDSProd To MSDQA",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "@if(empty(pipeline().parameters.Query),\nconcat('SELECT * FROM ',pipeline().parameters.SourceSchema,'.',pipeline().parameters.ScourceTableName),pipeline().parameters.Query)",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"parallelCopies": 2,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SqlServerSink",
						"type": "DatasetReference",
						"parameters": {
							"schema": {
								"value": "@pipeline().parameters.SourceSchema",
								"type": "Expression"
							},
							"table": {
								"value": "@pipeline().parameters.ScourceTableName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SqlServerSink_QA",
						"type": "DatasetReference",
						"parameters": {
							"schema": {
								"value": "@pipeline().parameters.TargetSchema",
								"type": "Expression"
							},
							"table": {
								"value": "@pipeline().parameters.TargetTableName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"SourceSchema": {
				"type": "string"
			},
			"ScourceTableName": {
				"type": "string"
			},
			"TargetSchema": {
				"type": "string",
				"defaultValue": "dbo"
			},
			"TargetTableName": {
				"type": "string"
			},
			"Query": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Miscellaneous Pipelines"
		},
		"annotations": []
	}
}